{
    "name": "olakunlevpn/mages",
    "description": "Mages preset for Laravel.",
    "keywords": [
        "alpine",
        "alpinejs",
        "bootstrap",
        "bootstrap 5",
        "laravel",
        "livewire",
        "preset"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/olakunlevpn/mages/issues",
        "source": "https://github.com/olakunlevpn/mages/"
    },
    "authors": [
        {
            "name": "Olakunle",
            "email": "olakunlevpn@live.com"
        }
       
    ],
    "require": {
        "php": "^8.0",
        "laravel/ui": "^4.2",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Olakunlevpn\\Mages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olakunlevpn\\Tall\\Mages\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Olakunlevpn\\Mages\\MagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
